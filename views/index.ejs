<!DOCTYPE html>
<html>
  <head>
    <title>homepage</title>
    <%- include head %>
  </head>
  <body>
    <%- include('bar', {username: username}) %>

    <div class="list">

      <% list.map(function(item,index){ %>
        <div class="row">
            <span><%= index+1 %></span>
            <span><%= item.username %></span>
            <span><%= item.title %></span>
            <span><%= item.time %></span>
            <span>
              <a href="">Edit</a>
              <a href="">Delete</a>
            </span>
        </div>
      <% }) %>
      
      
    </div>

    
  </body>
</html>
